{
  "expo": {
    "name": "Internet Of Tsiken",
    "slug": "capstone",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Internet of Tsiken to save PDF reports to your device.",
          "savePhotosPermission": "Allow Internet of Tsiken to save PDF reports to your Downloads folder.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-font"
    ],
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "nativeVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0f74e60b-9e1c-4f77-b863-a1c9a46b25ba"
    },
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ailynmay.capstone"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ailynmay.capstone",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0f74e60b-9e1c-4f77-b863-a1c9a46b25ba"
      }
    },
    "scheme": "internet-of-tsiken"
  }
}
