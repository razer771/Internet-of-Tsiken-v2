{
  "expo": {
    "name": "Internet Of Tsiken",
    "slug": "capstone",
    "plugins": [
      "expo-video",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Internet of Tsiken to save PDF reports to your device.",
          "savePhotosPermission": "Allow Internet of Tsiken to save PDF reports to your Downloads folder.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ailynmay.capstone"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0f74e60b-9e1c-4f77-b863-a1c9a46b25ba"
      }
    },
    "scheme": "internet-of-tsiken"
  }
}
